{
  "esdatasource": {
    "name": "esdatasource",
    "settings": {
      "indexName": "testormuser",
      "isAlias": false,
      "isPattern": false,
      "mappings": {
        "properties": {
          "docType": {
            "type": "keyword"
          },
          "userId": {
            "type": "keyword"
          },
          "username": {
            "type": "keyword"
          },
          "tenantId": {
            "type": "keyword"
          },
          "firstname": {
            "type": "keyword"
          },
          "lastname": {
            "type": "keyword"
          },
          "roles": {
            "type": "keyword"
          },
          "roleId": {
            "type": "keyword"
          },
          "roleName": {
            "type": "keyword"
          },
          "departmentId": {
            "type": "keyword"
          },
          "departmentName": {
            "type": "keyword"
          },
          "email": {
            "type": "keyword"
          },
          "mobile": {
            "type": "keyword"
          },
          "mfa": {
            "type": "object",
            "enabled": false
          },
          "consent": {
            "type": "object",
            "enabled": false
          },
          "isSecondaryUser": {
            "type": "boolean"
          },
          "secondaryUserInfo": {
            "type": "object",
            "enabled": false
          },
          "userStoreDomain": {
            "type": "keyword"
          }
        }
      },
      "aliases": {},
      "indexSettings": {
        "number_of_shards": 1,
        "number_of_replicas": 1
      },
      "createIndex": true,
      "updateMapping": true,
      "version": 7,
      "defaultLimit": 200,
      "configuration": {
        "nodes": [
          "https://localhost:9200"
        ],
        "requestTimeout": 30000,
        "pingTimeout": 3000,
        "auth": {
          "username": "admin",
          "password": "admin"
        },
        "agent": {
          "maxSockets": 1,
          "keepAlive": false
        },
        "ssl": {
          "rejectUnauthorized": false
        },
        "sniffInterval": 10000
      }
    },
    "connector": "query-orm-connector-elastic",
    "localConnector": false
  }
}
